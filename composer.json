{
    "name": "panvagenas/wp-plugin-core",
    "version": "0.0.1-dev",
    "description": "WP Plugins Core for developing plugins",
    "license": "GPL-3.0+",
    "keywords": [],
    "authors": [
        {
            "name": "Panagiotis Vagenas",
            "email": "pan.vagenas@gmail.com",
            "homepage": "https://gr.linkedin.com/in/panvagenas",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "vierbergenlars/php-semver": "3.*",
        "monolog/monolog": "1.*",
        "rmccue/requests": "1.*",
        "danielstjules/stringy": "2.*",
        "nesbot/carbon": "1.*",
        "aura/session": "2.*",
        "ircmaxell/random-lib": "1.*",
        "filp/whoops": "1.1.7",
        "respect/validation": "0.9.*"
    },
    "require-dev": {
        "symfony/var-dumper": "2.7.*",
        "devster/ubench": "1.*",
        "mikey179/vfsStream": "~1"
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "files": [
            "resources/includes/redux/redux-framework/framework.php",
            "resources/includes/redux/redux-extensions/extensions-init.php"
        ],
        "psr-4": {
            "WPluginCore002\\": "src/WPluginCore"
        }
    },
    "archive": {
        "exclude": [
            "/docs",
            "/tests"
        ]
    }
}
